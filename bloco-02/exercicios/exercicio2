import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;
import java.util.stream.Collectors;

public class RegistroTransacoesComStream {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Lê o saldo inicial da conta
        double saldo = scanner.nextDouble();

        // Lê a quantidade total de transações
        int quantidadeTransacoes = scanner.nextInt();

        List<Transacao> transacoes = new ArrayList<>();

        // Processa cada transação
        for (int i = 1; i <= quantidadeTransacoes; i++) {
            // Lê o tipo de transação (D para depósito ou S para saque)
            char tipoTransacao = scanner.next().charAt(0);
            // Lê o valor da transação
            double valorTransacao = scanner.nextDouble();

            // Cria uma nova transação e adiciona à lista de transações
            Transacao transacao = new Transacao(tipoTransacao, valorTransacao);
            transacoes.add(transacao);

            // Atualiza o saldo da conta com base no tipo de transação
            if (transacao.getTipo() == 'd') {
                saldo += valorTransacao; // Depósito aumenta o saldo
            } else if (transacao.getTipo() == 's') {
                saldo -= valorTransacao; // Saque diminui o saldo
            }
        }

        // Exibe o saldo final da conta
        System.out.println("Saldo : " + saldo);

        // Exibe as transações formatadas conforme o exemplo
        System.out.println("Transacoes:");
        transacoes.stream()
                .map(transacao -> transacao.getTipo() + " de " + transacao.getValor())
                .forEach(System.out::println);

        // Fechar o scanner para evitar vazamentos de recursos
        scanner.close();
    }
}

class Transacao {
    private char tipo;
    private double valor;

    public Transacao(char tipo, double valor) {
        this.tipo = tipo;
        this.valor = valor;
    }

    public char getTipo() {
        return tipo;
    }

    public double getValor() {
        return valor;
    }
}
